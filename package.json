{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 9"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "test:unit:watch": "vitest",
    "test:unit": "vitest --run",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e": "playwright test",
    "lint": "pnpm lint:es && pnpm lint:style",
    "lint:es": "eslint --flag unstable_ts_config --max-warnings 0",
    "lint:style": "stylelint  \"**/*.{css,less,scss,sass,pcss,postcss,sss,vue}\"",
    "lint:fix": "pnpm lint:es:fix && pnpm lint:style:fix",
    "lint:es:fix": "pnpm lint:es --fix",
    "lint:style:fix": "pnpm lint:style --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@nuxt/eslint": "^0.6.0",
    "@nuxt/fonts": "^0.10.0",
    "nuxt": "^3.13.2",
    "vue": "latest"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^5.1.1",
    "@nuxt/test-utils": "^3.14.4",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@playwright/test": "^1.48.1",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/lint-staged": "^13.3.0",
    "@vue/language-plugin-pug": "^2.1.6",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-vue-pug": "^0.6.2",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "jiti": "^2.3.3",
    "lint-staged": "^15.2.10",
    "postcss-html": "^1.7.0",
    "postcss-sass": "^0.5.0",
    "pug": "^3.0.3",
    "sass-embedded": "^1.80.3",
    "std-env": "^3.7.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
